{
  "jupyter_images": [
{% for image in images %}
    {
      "name": "{{ image.name }}",
      "registry": "{{ harbor_registry }}",
      "project": "library",
      "tag": "latest",
      "full_path": "{{ harbor_registry }}/library/{{ image.name }}:latest",
      "gpu_required": {{ image.gpu_required | lower }},
      "description": "{{ image.description }}",
      "category": "jupyter-compatible",
      "metadata": {
        "jupyter_compatible": true,
        "gpu_required": {{ image.gpu_required | lower }},
{% if image.name == 'jupyter-ml-cpu' %}
        "packages": ["scikit-learn", "pandas", "matplotlib", "transformers"],
        "memory_recommended": "8G"
{% elif image.name == 'jupyter-ml-gpu' %}
        "packages": ["torch", "transformers", "accelerate", "tensorboard"],
        "memory_recommended": "16G"
{% elif image.name == 'jupyter-fine-tuning' %}
        "packages": ["unsloth", "trl", "peft", "bitsandbytes", "deepspeed"],
        "memory_recommended": "32G"
{% elif image.name == 'jupyter-agent-dev' %}
        "packages": ["langchain", "crewai", "chromadb", "langserve"],
        "memory_recommended": "8G"
{% endif %}
      }
    }{% if not loop.last %},{% endif %}
{% endfor %}
  ]
}